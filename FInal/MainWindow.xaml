<Window x:Class="FInal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FInal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid HorizontalAlignment="Center" Height="291" Margin="0,98,0,0" VerticalAlignment="Top" Width="729">
            <Grid HorizontalAlignment="Left" Height="119" Margin="123,0,0,0" VerticalAlignment="Top" Width="458">
                <Label Content="Busca tu zona de interes" HorizontalAlignment="Center" VerticalAlignment="Top" FontWeight="Bold" FontSize="48" Height="63" FontFamily="Bernard MT Condensed"/>
                <TextBox x:Name="Buscador" TextWrapping="Wrap" TextChanged="TextBox_TextChanged" Text="Buscar..." FontSize="22" GotFocus="Buscador_GotFocus" LostFocus="Buscador_LostFocus" Width="268" HorizontalAlignment="Center" Margin="0,78,0,10"/>
                <Button Content="Button" Foreground="{x:Null}" RenderTransformOrigin="0.273,-1.145" HorizontalAlignment="Left" Width="40" Height="20" Click="Button_Click_1" VerticalAlignment="Top" Margin="323,85,0,0">
                    <Button.BorderBrush>
                        <ImageBrush/>
                    </Button.BorderBrush>
                    <Button.Background>
                        <ImageBrush ImageSource="/search_7079548.png"/>
                    </Button.Background>
                </Button>
            </Grid>
            <Grid HorizontalAlignment="Left" Height="73" Margin="0,218,0,0" VerticalAlignment="Top" Width="139">
                <Button Content="Button" HorizontalAlignment="Left" Margin="38,33,0,0" VerticalAlignment="Top" Height="40" Width="60" Click="Button_Click">
                    <Button.Foreground>
                        <ImageBrush/>
                    </Button.Foreground>
                    <Button.BorderBrush>
                        <ImageBrush/>
                    </Button.BorderBrush>
                    <Button.Background>
                        <ImageBrush ImageSource="/8673568_ic_fluent_open_folder_iconfinder-_1_.png"/>
                    </Button.Background>
                </Button>
                <Label Content="Ver grafico zonas" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Opacity="0.75"/>
                <Label Content="Ver grafico zonas" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="16" FontWeight="Bold" Opacity="0.75" Margin="223,0,0,0"/>
            </Grid>
            <Button Content="Button" HorizontalAlignment="Left" Height="29" Margin="220,77,0,0" VerticalAlignment="Top" Width="218" Visibility="Hidden"/>
            <Label x:Name="mensaje" Content="" HorizontalAlignment="Left" Height="130" Margin="162,124,0,0" VerticalAlignment="Top" Width="481" FontSize="36" FontFamily="Cambria" FontWeight="Bold" Foreground="#FFE21212" Opacity="0.5"/>
            <Button Content="Button" HorizontalAlignment="Left" Height="37" Margin="624,254,0,0" VerticalAlignment="Top" Width="44" Click="Button_Click_2" Foreground="{x:Null}" BorderBrush="{x:Null}">
                <Button.Background>
                    <ImageBrush ImageSource="/info_471662.png"/>
                </Button.Background>
            </Button>
            <Label Content="Más Información" HorizontalAlignment="Left" Height="40" Margin="577,218,0,0" VerticalAlignment="Top" Width="152" FontSize="16" FontWeight="Bold" Opacity="0.75"/>
        </Grid>
    </Grid>
</Window>
