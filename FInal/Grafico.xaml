<Window x:Class="FInal.Grafico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FInal"
        xmlns:lvc ="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="Grafico" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Series="{Binding SeriesCollection}" LegendLocation="Bottom" Margin="28,39,28,24">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Zonas" Labels="{Binding Labels}"/>
            </lvc:CartesianChart.AxisX>

            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Cant. de lugares"></lvc:Axis>
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
        <Button Content="Button" HorizontalAlignment="Left" Height="39" Margin="10,0,0,0" VerticalAlignment="Top" Width="44" Foreground="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click" Cursor="None">
            <Button.Background>
                <ImageBrush ImageSource="/WhatsApp Image 2025-11-21 at 21.52.11.jpeg"/>
            </Button.Background>
        </Button>
        <Label Content="Muy bueno" HorizontalAlignment="Left" Height="40" Margin="673,340,0,0" VerticalAlignment="Top" Width="95" Opacity="0.4" FontSize="10"/>
        <Label Content="Ligeramente Bueno" HorizontalAlignment="Left" Height="40" Margin="421,340,0,0" VerticalAlignment="Top" Width="124" Opacity="0.4" FontSize="10"/>
        <Label Content="Malo" HorizontalAlignment="Left" Height="40" Margin="210,340,0,0" VerticalAlignment="Top" Width="41" Opacity="0.4" FontSize="10"/>
        <Label Content="Gráfico Zonas" HorizontalContentAlignment="Center" HorizontalAlignment="Left" Height="32" Margin="222,7,0,0" VerticalAlignment="Top" Width="386" FontFamily="Bahnschrift" FontSize="20"/>
    </Grid>
</Window>